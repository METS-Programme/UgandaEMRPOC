          formAddMetadata="yes"
          formUILocation="patientDashboard.visitActions"
          formOrder="7"
          formIcon="icon-medkit"
          formShowIf="(visit.active || !visit.active) &amp;&amp; patient.person.gender=='M' &amp;&amp; patient.person.dead==false &amp;&amp; sessionLocation.uuid=='8f96e239-8586-4ec6-9375-04c6e19628ae'"
          formDisplayStyle="Standard"
          formLabel="HMIS ACP 029: Safe Male Circumcision Client Form">

  <script type="text/javascript">
    if (jQuery) {

      jq(document).ready(function () {
        //Disable encounter date field
        disableEncounterDate();

        //Block Multiple encounter entry on same date
        blockEncounterOnSameDateEncounter(jq("#encounterDate").find("input[type=hidden]"),"block");

        disable_fields("specify-other-smc-info-source");
        disable_fields("specify-other-entry-point");
        disable_fields("adverse-event-presence");
        disable_fields("specify-other-ae");
        disable_fields("circumcision-procedure-card");
        disable_fields("circumcision-procedure");
        disable_fields("post-operation-management-card")
        disable_fields("smc_client_in_care");
        disable_fields("other-circumcision-procedure");
        disable_fields('ring-size');

        <!-- disable other source of smc info -->
        jq('#other-smc-info-source').click(function () {
          if (getValue('other-smc-info-source.value') == 90002) {
              enable_fields('specify-other-smc-info-source');
          } else {
              disable_fields('specify-other-smc-info-source');
          }
        });

        <!-- disable other care entry point -->
        jq('#other-entry-point').click(function () {
          if (getValue('other-entry-point.value') == 90002) {
              enable_fields('specify-other-entry-point');
          } else {
              disable_fields('specify-other-entry-point');
          }
        });

        <!-- disable hiv care or referred field if not HIV positive -->
        jq('#hiv-test-results').change(function () {
          if (getValue('hiv-test-results.value') == 703) {
              enable_fields("smc_client_in_care");
          } else {
              disable_fields("smc_client_in_care");
          }
        });

        <!-- disable other presence of AE    -->
        jq('#smc_presence_of_ae').click(function () {
          if (getValue('smc_presence_of_ae.value') == 90002) {
              enable_fields('smc_presence_of_ae_other');
          } else {
              disable_fields('smc_presence_of_ae_other');
          }
        });

        <!-- enable ring size field if shang ring or enable other procedure if procedure not in the list  -->
        jq('#circumcision-procedure').change(function () {
          if (getValue('circumcision-procedure.value') == 162911) {
              enable_fields('ring-size');
          } else if (getValue('circumcision-procedure.value') == 90002) {
              enable_fields("other-circumcision-procedure");
              disable_fields('ring-size');
          } else {
              disable_fields('ring-size');
              disable_fields("other-circumcision-procedure");
          }
        });

        <!-- enable adverse event card if any adverse events occurred-->
        jq('#adverse-events').click(function () {
          if (getValue('adverse-events.value') == 90003) {
              enable_fields("adverse-event-presence");
          } else {
              disable_fields("adverse-event-presence");
          }
        });

        <!-- enable other ae field if adverse event not listed -->
        jq('#other-ae').click(function () {
          if(jq(this).find(':checkbox').prop('checked')) {
              enable_fields("specify-other-ae");
          } else {
              disable_fields("specify-other-ae");
          }
        });

        <!-- activate circumcision procedure and post operation management cards if eligibility conditions are met -->
        var eligibilityValues = {};

        jq('.eligibility').find("input[type$='radio']").change(function (event) {
          eligibilityValues[event.target.name] = event.target.value;
          var eligibilityList =  Object.keys(eligibilityValues).filter(function(data) {
          <!-- add to eligibility list if yes -->
          if (eligibilityValues[data] === '90003') {
            return true;
          }
            return false;
          });

          if (eligibilityList.length === 3) {
              enable_fields("circumcision-procedure-card");
              enable_fields("post-operation-management-card")
          } else {
              disable_fields("circumcision-procedure-card");
              disable_fields("post-operation-management-card")
          }
        });

        <ifMode mode="EDIT">
          <!-- If eligibility values are pre-checked, activate circumcision procedure and post operation management cards -->
          var i = 0;
          jq('.eligibility').find("input[type$='radio']").each(function () {
            eligibilityValues[i] = this.value;
            i++;
            var eligibilityList =  Object.keys(eligibilityValues).filter(function(data) {
            <!-- add to eligibility list if yes -->
            if (eligibilityValues[data] === '90003') {
              return true;
            }
              return false;
            });

            if (eligibilityList.length === 3) {
              enable_fields("circumcision-procedure-card");
              enable_fields("post-operation-management-card")
            } else {
              disable_fields("circumcision-procedure-card");
              disable_fields("post-operation-management-card")
            }
          });
        </ifMode>

        <!-- Enabling and Disabling Prescription of multiple drugs -->
          $allRows = jq(this).find('.drugs-prescribed');
          /*hide default prescription rows on load*/
          $allRows.hide();
          jq('#1-removeEntry').remove();
          jq('#10-addEntry').remove();
          jq('#1-toggleContainer').show();

          jq('#11-removeEntry').remove();
          jq('#20-addEntry').remove();
          jq('#11-toggleContainer').show();

          jq('.addEntry').on("click", function() {
              var correctedAddButtonId = parseFloat(this.id) + 1;
              var contentAddId = "#" + correctedAddButtonId + "-toggleContainer";
              jq(contentAddId).toggle(true);
              jq('#' + this.id).toggle(false); jq('#' + parseFloat(this.id) + '-removeEntry').toggle(true);
              return false;
          });

          jq('.removeEntry').on("click", function() {
              var correctedRemoveButtonId = parseFloat(this.id) - 1;
              var contentAddId = "#" + parseFloat(this.id) + "-toggleContainer";
              jq(contentAddId).toggle(false);
              jq( ':input:not(:button)', contentAddId).val([]);
              jq('#' + correctedRemoveButtonId + '-addEntry').toggle(true); jq('#' + correctedRemoveButtonId + '-removeEntry').toggle(true);

              return false;
          });
      });
    }
  </script>

