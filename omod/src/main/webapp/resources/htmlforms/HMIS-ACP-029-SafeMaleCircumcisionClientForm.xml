                    <!-- measurements -->
                    <div class="card">
                      <div class="card-body">
                        <div class="row ">
                          <div class="col-md-2">
                            <label>BP</label>
                            <div class="col-md-12  align-items-center row">
                              <obs conceptId="CIEL:5085" id="bp_systolic" />
                              <p>/</p>
                              <obs conceptId="CIEL:5086" id="bp_diastolic" />
