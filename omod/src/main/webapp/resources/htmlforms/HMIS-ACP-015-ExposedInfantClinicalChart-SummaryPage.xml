        <lookup complexExpression="
            #set( $encounter = 0 )
            #set( $encounter = $fn.latestEncounter('9fcfcc91-ad60-4d84-9710-11cc25258719'))
            #if( $encounter != 0 )
                &lt;script type=&quot;text/javascript&quot;>
