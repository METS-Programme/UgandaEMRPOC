    <script type="text/javascript">
        
        if (jQuery) {

            jq(document).ready(function () {

                //Block Multiple encounter entry on same date
                blockEncounterOnSameDateEncounter(jq("#encounterDate").find("input[type=hidden]"),"block");

                // Validate MUAC
                 var age  = <lookup complexExpression="$patient.age" />; 
                 getMUACCodeFromMUACScoreByAge(age, "muac-score", "muac-code");

                //Disable encounter date field
                disableEncounterDate();

                disable_fields("99431");        // partner pre art number
                disable_fields("165385");      // stk/facility results
                disable_fields("165422")      // tfv for partner
                disable_fields("90203");     // who clinical stage
                disable_fields("5497");     //cd4 count
                disable_fields("159376");  //cd4 date
                disable_fields("163023"); //viral load date
                disable_fields("1305");  // viral load results
                disable_fields("856");  //viral load copies
                <!-- disable_fields("other-facility-referred-from");
                disable_fields("other-facility-referred-to"); -->


                jq("#woman-emtct-code").change(function () {
                if(jq(this).find("select").find(":selected").val() == "99314") {
                jq("#tfv-woman").find("input[type$='radio']").attr("disabled", false);
                } else if(jq(this).find("select").find(":selected").val() == "99739") {
                jq("#tfv-woman").find("input[type$='radio']").attr("disabled", false);
                } else if (jq(this).find("select").find(":selected").val() == "99736") {
                jq("#tfv-woman").find("input[type$='radio']").attr("disabled", false);
                } else {
                jq("#tfv-woman").find("input[type$='radio']").attr("disabled", true);
                }
                });

                jq("#partner-emtct-code").change(function () {
                if(jq(this).find("select").find(":selected").val() == "99314") {
                jq("#tfv-partner").find("input[type$='radio']").attr("disabled", false);
                } else if (jq(this).find("select").find(":selected").val() == "99739") {
                jq("#tfv-partner").find("input[type$='radio']").attr("disabled", false);
                } else if (jq(this).find("select").find(":selected").val() == "99736") {
                jq("#tfv-partner").find("input[type$='radio']").attr("disabled", false);
                } else {
                jq("#tfv-partner").find("input[type$='radio']").attr("disabled", true);
                }
                });

                var gravidaValidate = function() {
                var gravidaField = getField('gravida.value');
                var gravida = getValue('gravida.value');
                var gravidaErrorField = getField('gravida.error');

                if (gravida &lt; 0) {
                    gravidaField.addClass('error');
                    gravidaErrorField.html('Parity cannot be less than 0').show();
                    return false;
                } else {
                    gravidaField.removeClass('error');
                    gravidaErrorField.html('').hide();
                }

                return true;
                };

                var parityValidate = function() {
                    var parityField = getField('parity.value');
                    var parity = getValue('parity.value');
                    var parityErrorField = getField('parity.error');

                    if (parity &lt; 0) {
                        parityField.addClass('error');
                        parityErrorField.html('Gravida cannot be less than 0').show();
                        return false;
                    } else {
                        parityField.removeClass('error');
                        parityErrorField.html('').hide();
                    }

                    return true;
                };

                var gestationalAgeValidate = function() {
                    var gestationalAge = getValue('gestational-age.value');
                    var gestationalAgeErrorField = getField('gestational-age.error');

                    if (gestationalAge &gt; 45) {
                        gestationalAgeField.addClass('error');
                        gestationalAgeErrorField.html('Gestational Age cannot be greater than 45').show();
                        return false;
                    } else {
                        gestationalAgeField.removeClass('error');
                        gestationalAgeErrorField.html('').hide();
                    }

                    return true
                }
          
            });
        }

    </script>

