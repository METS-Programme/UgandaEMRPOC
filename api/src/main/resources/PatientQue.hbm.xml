<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.openmrs.module.ugandaemrpoc.model">

	<class name="PatientQueue" table="patient_queue">

        <id name="id" type="int" column="patient_queue_id" unsaved-value="0">
            <generator class="native" />
        </id>

		<many-to-one name="patient" class="org.openmrs.Patient" column="patient_id"/>

		<many-to-one name="provider" class="org.openmrs.Provider" column="provider_id"/>

		<many-to-one name="locationFrom" class="org.openmrs.Location" column="location_from" />

		<many-to-one name="locationTo" class="org.openmrs.Location" column="location_to" />

		<property name="status" type="java.lang.String" column="status" />

		<property name="dateCreated" type="java.util.Date" column="date_created"
				  not-null="true" />

		<many-to-one name="creator" class="org.openmrs.User" not-null="true" />

		<property name="dateChanged" type="java.util.Date" column="date_changed" />

		<many-to-one name="changedBy" column="changed_by" class="org.openmrs.User" />

		<property name="voided" column="voided" />

		<property name="dateVoided" type="java.util.Date" column="date_voided"/>

		<many-to-one name="voidedBy" column="voided_by" class="org.openmrs.User" />

		<property name="voidReason" type="java.lang.String" column="void_reason" />
			
	</class>
</hibernate-mapping>
